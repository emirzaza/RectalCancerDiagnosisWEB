
@{
    ViewData["Title"] = "Mainpage";
}

<h2>ADD MRI @ViewBag.UserId </h2>

<form >
    <div class="mb-3">
        <label>Name your mri</label>
        <br />
        <input type ="text" />
        <br />
        <label for="formFile" class="form-label">Choose the mri file</label>
        <br />
        <input class="form-control" type="file" id="formFile" name="image" required>
    </div>
    
    <button type="submit" class="btn btn-primary">SUBMIT</button>
</form>